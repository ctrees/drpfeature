{
  "name": "drpfeature",
  "version": "0.0.1",
  "description": "Boilerplate project to run drp UX feature tests with Cucumber",
  "homepage": "https://github.com/ctrees/drpfeature",
  "scripts": {
    "clean": "read -p \"Are you sure you want to clean the project? [y/n] \" REPLY; if [[ $REPLY =~ ^[Yy]$ ]]; then rm -R .git .github demo-app test .codeclimate.yml .travis.yml jest.json wdio.BUILD.conf.js src/features/**; fi",
    "test": "yarn run test:features",
    "test:features": "wdio wdio.BUILD.conf.js; wdio_ret=$?; forever stopall; exit $wdio_ret",
    "test:unit": "jest --config=jest.json",
    "test:validate": "eslint 'src/**/*.js' 'test/**/*.js!(setup.js)'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctrees/drpfeature.git"
  },
  "bugs": {
    "url": "https://github.com/ctrees/drpfeature/issues"
  },
  "keywords": [
    "drp",
    "drpfeature",
    "webdriverio",
    "cucumber",
    "test",
    "selenium"
  ],
  "author": "Chris Trees <ctrees@mailserviceslc.com>",
  "contributors": [
    "Chris Trees <christrees@yahoo.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "~6.24.0",
    "babel-register": "~6.26.0",
    "chai": "~4.1.2",
    "wdio-cucumber-framework": "~1.0.2",
    "wdio-phantomjs-service": "~0.2.2",
    "wdio-selenium-standalone-service": "~0.0.9",
    "wdio-spec-reporter": "~0.1.2",
    "webdriverio": "4.9.8"
  },
  "devDependencies": {
    "babel-jest": "~22.0.0",
    "babel-polyfill": "~6.26.0",
    "eslint": "~4.14.0",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-import": "~2.8.0",
    "forever": "~0.15.3",
    "http-server": "~0.10.0",
    "jest": "~22.0.0"
  }
}
